using UnityEngine;
using System.Collections;

public class FrontDeskSpriteOrderManager : SpriteOrderLayerManager {

	protected override void LayerChangeWhenPlayerIsOverThisObject(){
		this.renderer.sortingOrder = 1;
	}
	
	protected override void LayerChangeWhenPlayerisUnderThisObject(){
		this.renderer.sortingOrder = originalSortingOrder;
	}
}
