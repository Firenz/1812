using UnityEngine;
using System.Collections;
using System.Collections.Generic;

public class LittleFlags : InteractiveObject {

	protected override void InitializeInformation (){
		currentType = interactiveTypes.bottomPickable;
		definitionText.Add("Me pregunto que");
		definitionText.Add("sera esto del");
		definitionText.Add("suelo");
		interactionText.Add("Voy a cogerlo");
		interactionText.Add("para ver que es");
		List<string> _dialogueAfterPickingObject = new List<string>();
		_dialogueAfterPickingObject.Add("Vaya parece ser");
		_dialogueAfterPickingObject.Add("un objeto para");
		_dialogueAfterPickingObject.Add("un puzzle pero");
		_dialogueAfterPickingObject.Add("como no esta");
		_dialogueAfterPickingObject.Add("implementado paso de");
		_dialogueAfterPickingObject.Add("decir que es");
		givableObject = new GivableObject("LittleFlagsInventory_v1", _dialogueAfterPickingObject);
	}
}
